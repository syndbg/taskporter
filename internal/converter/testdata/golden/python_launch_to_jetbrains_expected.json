    {
      "XMLName": {
        "Space": "",
        "Local": ""
      },
      "Name": "WORKING_DIRECTORY",
      "Value": "$PROJECT_DIR$"
    }
  ],
  "EnvVars": {
    "XMLName": {
      "Space": "",
      "Local": ""
    },
    "EnvVars": [
      {
        "XMLName": {
          "Space": "",
          "Local": ""
        },
        "Name": "DEBUG",
        "Value": "1"
      },
      {
        "XMLName": {
          "Space": "",
          "Local": ""
        },
        "Name": "PYTHONPATH",
        "Value": "$PROJECT_DIR$/src"
      }
    ]
  }
}
