{
  "XMLName": {
    "Space": "",
    "Local": ""
  },
  "Name": "Launch Python App",
  "Type": "PythonConfigurationType",
  "Options": [
    {
      "XMLName": {
        "Space": "",
        "Local": ""
      },
      "Name": "SCRIPT_NAME",
      "Value": "$PROJECT_DIR$/src/main.py"
    },
    {
      "XMLName": {
        "Space": "",
        "Local": ""
      },
      "Name": "PARAMETERS",
      "Value": "${workspaceFolder}/src/main.py --verbose --config settings.ini"
