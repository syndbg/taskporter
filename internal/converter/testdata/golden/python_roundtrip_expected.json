{
  "name": "Launch Python App",
  "type": "python",
  "request": "launch",
  "program": "${workspaceFolder}/src/main.py",
  "args": [
    "$PROJECT_DIR$/src/main.py",
    "${workspaceFolder}/src/main.py",
    "--verbose",
    "--config",
    "settings.ini"
  ],
  "cwd": "${workspaceFolder}",
  "env": {
    "DEBUG": "1",
    "PYTHONPATH": "${workspaceFolder}/src"
  }
}
